    "imperfection",
    "organic",
    "clip-path",
    "polygon",
    "design",
    "ui",
    "visual"
  ],
  "author": "Michail Rybakov <michail@rybakov.com> (http://rybakov.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rybi/wabi.js.git"
  },
  "homepage": "https://github.com/rybi/wabi.js#readme",
  "bugs": {
    "url": "https://github.com/rybi/wabi.js/issues"
  },
  "devDependencies": {
    "rollup": "^4.9.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-node-resolve": "^15.2.3"
  }
}
