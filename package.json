{
  "name": "wabijs",
  "version": "0.1.0",
  "type": "module",
  "description": "A lightweight JavaScript library for adding organic geometric imperfection to HTML elements",
  "main": "dist/wabi.cjs.js",
  "module": "dist/wabi.esm.js",
  "browser": "dist/wabi.min.js",
  "unpkg": "dist/wabi.min.js",
  "types": "dist/wabi.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "demo": "npx serve ."
  },
  "keywords": [
